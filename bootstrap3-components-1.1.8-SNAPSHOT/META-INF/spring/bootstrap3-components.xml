<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="columnInitializer" class="org.jahia.initializers.ColumnInitializer">
        <property name="key" value="columnInitializer" />
    </bean>

    <bean id="sliderInitializer" class="org.jahia.initializers.SliderInitializer">
        <property name="key" value="sliderInitializer" />
    </bean>

</beans>